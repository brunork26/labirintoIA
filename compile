if [[ ( $1 == "run" ) ]];
then
    rm -rf *.class *~
    javac Inicio.java
    javac LeitorDeArquivo.java
    javac Labirinto.java
    echo "Iniciando projeto"
    java Inicio 
else
    if [[ ( $1 == "clean" ) ]];
        then  
            rm -rf *.class *~
            echo "Projeto Limpo."
        else 
            rm -rf *.class *~
            javac Inicio.java
            javac LeitorDeArquivo.java
            javac Labirinto.java
            echo "Projeto Compilado."
    fi
fi

